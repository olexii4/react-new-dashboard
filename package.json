{
  "name": "react-new-dashboard",
  "version": "1.0.0",
  "description": "New dashboard for Eclipse CHE",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olexii4/react-new-dashboard.git"
  },
  "author": "Oleksii Orel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/olexii4/react-new-dashboard/issues"
  },
  "homepage": "https://github.com/olexii4/react-new-dashboard#readme",
  "engines": {
    "yarn": ">=1.13.0",
    "node": ">=8.16"
  },
  "scripts": {
    "compile": "webpack --config webpack.config.prod.js",
    "compile:source-map": "yarn run compile --devtool source-map",
    "start": "webpack-dev-server --progress --color --config webpack.config.dev.js",
    "format": "tsfmt -r --useTsfmt tsfmt.json",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}'",
    "lint:fix": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --fix",
    "serve:prod": "webpack-dev-server --color --disable-host-check --config webpack.config.prod-dev.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@patternfly/patternfly": "^2.62.0",
    "@patternfly/react-core": "^3.140.11",
    "@patternfly/react-icons": "^3.15.3",
    "@patternfly/react-table": "^2.27.11",
    "axios": "^0.19.2",
    "connected-react-router": "^6.7.0",
    "gravatar-url": "^3.1.0",
    "history": "^4.10.1",
    "inversify": "^5.0.1",
    "inversify-react": "^0.4.3",
    "jquery": "^3.4.1",
    "js-yaml": "^3.13.1",
    "monaco-editor": "^0.20.0",
    "monaco-editor-core": "^0.14.6",
    "monaco-languageclient": "^0.12.0",
    "monaco-languages": "^1.8.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-loadable": "^5.5.0",
    "react-monaco-editor": "^0.34.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "reactstrap": "^8.4.1",
    "reconnecting-websocket": "^4.4.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "sockjs-client": "^1.4.0",
    "umd-compat-loader": "2.1.1",
    "yaml-language-server": "^0.7.2"
  },
  "devDependencies": {
    "@loadable/webpack-plugin": "^5.12.0",
    "@types/axios": "^0.14.0",
    "@types/history": "^4.7.5",
    "@types/jquery": "^3.3.32",
    "@types/js-yaml": "^3.12.2",
    "@types/less": "^3.0.1",
    "@types/node": "^13.7.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-gravatar": "^2.6.7",
    "@types/react-loadable": "^5.5.3",
    "@types/react-redux": "^7.1.7",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/reactstrap": "^8.4.1",
    "@types/vscode": "^1.44.0",
    "@types/webpack": "^4.41.6",
    "@types/webpack-dev-server": "^3.10.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.0",
