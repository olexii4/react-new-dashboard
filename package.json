    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "typescript-formatter": "^7.2.2",
    "webpack": "^4.41.5",
